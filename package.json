{
  "name": "@atlauncher/backend-next",
  "version": "0.1.0",
  "description": "Backend API server for ATLauncher NEXT.",
  "main": "dist",
  "scripts": {
    "build": "babel src -D -d dist",
    "clean": "atlauncher-scripts clean",
    "commit+": "git-cz",
    "commitmsg": "validate-commit-msg",
    "dev": "cross-env NODE_ENV=development nodemon -w src -e js,ejs,json --exec \"babel-node src/server.js\"",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=* nodemon -w src -e js,ejs,json --exec \"babel-node src/server.js\"",
    "lint": "atlauncher-scripts lint",
    "lint:js": "atlauncher-scripts lint js",
    "lint:js:watch": "npm run lint:js -- -watch",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "prepush": "npm run lint:js && npm run test",
    "seed": "knex seed:run",
    "start": "node dist/server.js",
    "test": "atlauncher-scripts test",
    "test:ci": "atlauncher-scripts test noConcurrency",
    "test:clean": "rimraf ./db/test.sqlite3",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/Backend-NEXT.git"
  },
  "author": {
    "name": "Ryan Dowling",
    "email": "ryan.dowling@atlauncher.com",
    "url": "https://github.com/RyanTheAllmighty"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ATLauncher/Backend-NEXT/issues"
  },
  "homepage": "https://github.com/ATLauncher/Backend-NEXT#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert",
        "WIP"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "app",
          "db",
          "docs",
          "test"
        ],
        "validate": true,
        "multiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "Commit message violates the rules defined for this project. Please, execute `npm run commit+` to generate a correct commit message with `commitizen`",
      "autoFix": false
    }
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cache-manager": "^2.5.0",
    "cache-manager-fs": "^1.0.6",
    "compression": "^1.7.1",
    "config": "^1.27.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-session-knex": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-rate-limit": "^2.9.0",
    "express-request-id": "^1.4.0",
    "express-request-version": "^2.1.2",
    "express-session": "^1.15.6",
    "helmet": "^3.9.0",
    "highlight.js": "^9.12.0",
    "http-status": "^1.0.1",
    "knex": "^0.13.0",
    "markdown-toc": "^1.2.0",
    "merge-deep": "^3.0.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.4.2",
    "oauth2-server": "^3.0.0",
    "oauth2orize": "^1.10.0",
    "objection": "^0.8.9",
    "passport": "^0.4.0",
    "passport-anonymous": "^1.0.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pug": "^2.0.0-rc.4",
    "remarkable": "^1.7.1",
    "remarkable-meta": "^1.0.1",
    "resource-router-middleware": "^0.7.0",
    "response-time": "^2.3.2",
    "uuid": "^3.1.0",
    "vantage": "^1.7.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@atlauncher/atlauncher-scripts": "0.1.0-15",
    "@atlauncher/babel-preset-atlauncher": "^0.1.0-11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.0",
    "cz-conventional-changelog": "^2.0.0",
    "faker": "^4.1.0",
    "get-port-sync": "^1.0.0",
    "husky": "^0.14.3",
    "knex-cleaner": "^1.1.3",
    "nodemon": "^1.12.1",
    "randexp": "^0.4.6",
    "rimraf": "^2.6.2",
    "sqlite3": "^3.1.13",
    "supertest": "^3.0.0",
    "validate-commit-msg": "^2.14.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8"
  }
}
