{
  "name": "@atlauncher/backend-next",
  "version": "0.1.0",
  "description": "Backend API server for ATLauncher NEXT.",
  "homepage": "https://github.com/ATLauncher/Backend-NEXT#readme",
  "bugs": {
    "url": "https://github.com/ATLauncher/Backend-NEXT/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ryan Dowling",
    "email": "ryan.dowling@atlauncher.com",
    "url": "https://github.com/RyanTheAllmighty"
  },
  "main": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/Backend-NEXT.git"
  },
  "scripts": {
    "build": "atlauncher-scripts build",
    "clean": "atlauncher-scripts clean",
    "commitmsg": "atlauncher-scripts commitmsg",
    "dev": "cross-env NODE_ENV=development nodemon -w src -e js,ejs,json --exec \"atlauncher-scripts babel-run src/server.js\"",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=* nodemon -w src -e js,ejs,json --exec \"atlauncher-scripts babel-run src/server.js\"",
    "lint": "npm-run-all lint:js lint:md lint:pj",
    "lint:js": "atlauncher-scripts lint:js",
    "lint:js:watch": "npm run lint:js -- -watch",
    "lint:md": "atlauncher-scripts lint:md",
    "lint:pj": "atlauncher-scripts lint:pj",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "prepush": "npm run lint:js && npm run test",
    "seed": "knex seed:run",
    "start": "node dist/server.js",
    "test": "atlauncher-scripts test",
    "test:ci": "atlauncher-scripts test --noConcurrency",
    "test:clean": "rimraf ./db/test.sqlite3",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-runtime": "~6.26.0",
    "bcryptjs": "~2.4.3",
    "body-parser": "~1.18.2",
    "cache-manager": "~2.5.0",
    "cache-manager-fs": "~1.0.6",
    "compression": "~1.7.1",
    "config": "~1.28.1",
    "connect-ensure-login": "~0.1.1",
    "connect-flash": "~0.1.1",
    "connect-session-knex": "~1.4.0",
    "cookie-parser": "~1.4.3",
    "cors": "~2.8.4",
    "date-fns": "~1.29.0",
    "ejs": "~2.5.7",
    "express": "~4.16.2",
    "express-rate-limit": "~2.9.0",
    "express-request-id": "~1.4.0",
    "express-request-version": "~2.2.0",
    "express-session": "~1.15.6",
    "helmet": "~3.9.0",
    "highlight.js": "~9.12.0",
    "http-status": "~1.0.1",
    "knex": "~0.13.0",
    "markdown-toc": "~1.2.0",
    "merge-deep": "~3.0.0",
    "mysql2": "~1.5.1",
    "oauth2-server": "~3.0.0",
    "oauth2orize": "~1.11.0",
    "objection": "~0.9.0",
    "passport": "~0.4.0",
    "passport-anonymous": "~1.0.1",
    "passport-http": "~0.3.0",
    "passport-http-bearer": "~1.0.1",
    "passport-local": "~1.0.0",
    "passport-oauth2-client-password": "~0.1.2",
    "pug": "~2.0.0-rc.4",
    "remarkable": "~1.7.1",
    "remarkable-meta": "~1.0.1",
    "resource-router-middleware": "~0.7.0",
    "response-time": "~2.3.2",
    "uuid": "~3.1.0",
    "vantage": "~1.7.0",
    "winston": "~2.4.0"
  },
  "devDependencies": {
    "@atlauncher/atlauncher-scripts": "~0.1.0-23",
    "cross-env": "~5.1.1",
    "faker": "~4.1.0",
    "get-port-sync": "~1.0.0",
    "husky": "~0.14.3",
    "knex-cleaner": "~1.1.3",
    "nodemon": "~1.12.1",
    "npm-run-all": "~4.1.2",
    "randexp": "~0.4.6",
    "rimraf": "~2.6.2",
    "sqlite3": "~3.1.13",
    "supertest": "~3.0.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "engineStrict": true
}
