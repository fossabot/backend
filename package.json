{
    "name": "@atlauncher/backend-api",
    "version": "0.1.0",
    "description": "A bunch of middleware for Koa in use by the ATLauncher backend",
    "homepage": "https://github.com/ATLauncher/backend",
    "bugs": {
        "url": "https://github.com/ATLauncher/backend/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Ryan Dowling",
        "email": "ryan.dowling@atlauncher.com",
        "url": "https://github.com/RyanTheAllmighty"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ATLauncher/backend.git"
    },
    "scripts": {
        "build": "atlauncher-scripts build",
        "clean": "atlauncher-scripts clean",
        "commitmsg": "atlauncher-scripts commitmsg",
        "db:migrate": "knex migrate:latest",
        "db:migrate:rollback": "knex migrate:rollback",
        "db:seed": "knex seed:run",
        "dev": "cross-env NODE_ENV=development nodemon",
        "lint": "npm-run-all lint:js lint:md lint:pj",
        "lint:js": "atlauncher-scripts lint:js",
        "lint:js:watch": "npm run lint:js -- -watch",
        "lint:md": "atlauncher-scripts lint:md",
        "lint:pj": "atlauncher-scripts lint:pj",
        "prepush": "npm-run-all lint test",
        "start": "atlauncher-scripts babel:run src/server.js",
        "test": "atlauncher-scripts test",
        "test:ci": "atlauncher-scripts test --noConcurrency",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch"
    },
    "dependencies": {
        "@atlauncher/atlauncher-scripts": "~0.1.0-23",
        "apollo-server-koa": "~1.3.2",
        "bcryptjs": "~2.4.3",
        "boom": "~7.1.1",
        "config": "~1.29.2",
        "date-fns": "~1.29.0",
        "glob": "~7.1.2",
        "graphql": "~0.12.3",
        "graphql-iso-date": "~3.3.0",
        "graphql-tools": "~2.19.0",
        "jsonwebtoken": "~8.1.1",
        "kcors": "~2.2.1",
        "knex": "~0.14.2",
        "koa": "~2.4.1",
        "koa-bodyparser": "~4.2.0",
        "koa-convert": "~1.2.0",
        "koa-helmet": "~3.3.0",
        "koa-json-error": "~3.1.2",
        "koa-jwt": "~3.3.0",
        "koa-passport": "~4.0.1",
        "koa-respond": "~1.0.1",
        "koa-response-time": "~2.0.0",
        "koa-router": "~7.4.0",
        "koa2-ratelimit": "~0.4.3",
        "objection": "~0.9.4",
        "passport-local": "~1.0.0",
        "uuid": "~3.2.1",
        "winston": "~2.4.0"
    },
    "devDependencies": {
        "cross-env": "~5.1.3",
        "get-port-sync": "~1.0.0",
        "husky": "~0.14.3",
        "nodemon": "~1.14.12",
        "npm-run-all": "~4.1.2",
        "sqlite3": "~3.1.13"
    },
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "engineStrict": true
}
